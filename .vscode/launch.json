	"version": "0.2.0",
	"configurations": [

    {
      "name": "whatsapp_clone",
      "cwd": "whatsapp_clone",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "whatsapp_clone (profile mode)",
      "cwd": "whatsapp_clone",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "whatsapp_clone (release mode)",
      "cwd": "whatsapp_clone",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Current Device",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "Zuk Z2",
      "request": "launch",
      "type": "dart",
      "deviceId": "192.168.0.70:5555"
    },
    {
      "name": "Chrome",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "args": [
        "--web-hostname 192.168.0.90",
        "--web-port 55555"
      ],
    }
  ],
	"compounds": [
        {
            "name": "All Devices",
            "configurations": ["Zuk Z2", "Chrome"],
        }
    ]
}